<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<BorderPane prefHeight="500.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/8.0.40" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.mmm.grenway.javafx.controller.ShowUsersController">
   <center>
      <TableView fx:id="usersTable" layoutX="21.0" layoutY="14.0" prefHeight="500.0" prefWidth="500.0" AnchorPane.bottomAnchor="50.0" AnchorPane.leftAnchor="50.0" AnchorPane.rightAnchor="50.0" AnchorPane.topAnchor="20.0">
        <columns>
          <TableColumn fx:id="userNameColumn" editable="false" prefWidth="75.0" text="%main.tab.admin.showusers.table.username" />
          <TableColumn fx:id="passwordColumn" editable="false" prefWidth="75.0" text="%main.tab.admin.showusers.table.password" />
            <TableColumn fx:id="rolesColumn" editable="false" prefWidth="75.0" text="%main.tab.admin.showusers.table.roles" />
            <TableColumn fx:id="accountNonExpiredColumn" editable="false" prefWidth="75.0" text="%main.tab.admin.showusers.table.accountNonExpired" />
            <TableColumn fx:id="accountNonLockedColumn" editable="false" prefWidth="75.0" text="%main.tab.admin.showusers.table.accountNonLocked" />
            <TableColumn fx:id="credentialsNonExpiredColumn" editable="false" prefWidth="75.0" text="%main.tab.admin.showusers.table.credentialsNonExpired" />
            <TableColumn fx:id="enabledColumn" editable="false" prefWidth="75.0" text="%main.tab.admin.showusers.table.enabled" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
         <BorderPane.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </BorderPane.margin>
      </TableView>
   </center>
   <top>
      <HBox prefHeight="50.0" spacing="10.0" BorderPane.alignment="CENTER">
         <children>
            <TextField fx:id="userNameFilter" />
            <ComboBox fx:id="rolesFilter" prefWidth="150.0" />
         </children>
         <padding>
            <Insets bottom="10.0" left="20.0" right="20.0" top="10.0" />
         </padding>
      </HBox>
   </top>
</BorderPane>
