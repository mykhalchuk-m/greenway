<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane xmlns="http://javafx.com/javafx/8.0.40" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.mmm.grenway.javafx.controller.InvitationDeliveryController">
   <children>
      <SplitPane dividerPositions="0.75" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
        <items>
          <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="160.0" prefWidth="100.0">
               <children>
                  <VBox prefHeight="160.0" prefWidth="58.0" spacing="20.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                     <children>
                        <TextField fx:id="filterUserName" />
                        <TableView fx:id="usersTable">
                          <columns>
                            <TableColumn fx:id="userNameColumn" prefWidth="75.0" text="%main.tab.inv.clientName" />
                            <TableColumn fx:id="birthDayColumn" prefWidth="75.0" text="%main.tab.inv.birthDay" />
                              <TableColumn fx:id="foringPassportNumberColumn" prefWidth="75.0" text="%main.tab.inv.foringPassportNumber" />
                              <TableColumn fx:id="regionColumn" prefWidth="75.0" text="%main.tab.inv.region" />
                              <TableColumn fx:id="registrationDateColumn" prefWidth="75.0" text="%main.tab.inv.registrationDate" />
                              <TableColumn fx:id="costColumn" prefWidth="75.0" text="%main.tab.inv.registrationDate" />
                              <TableColumn fx:id="invitationSupplierColumn" prefWidth="75.0" text="%main.tab.inv.invitationSupplier" />
                              <TableColumn fx:id="statusColumn" prefWidth="75.0" text="%main.tab.inv.status" />
                          </columns>
                           <columnResizePolicy>
                              <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                           </columnResizePolicy>
                        </TableView>
                     </children>
                     <padding>
                        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
                     </padding>
                  </VBox>
               </children>
            </AnchorPane>
          <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="160.0" prefWidth="100.0" />
        </items>
      </SplitPane>
   </children>
</AnchorPane>
